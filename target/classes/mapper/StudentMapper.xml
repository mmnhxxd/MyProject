<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sx.dao.StudentMappper">
    <insert id="insertStu" keyProperty="sid" useGeneratedKeys="true" parameterType="Student">
        insert into STUDENT(sname,spass) values (#{sname},#{spass})
    </insert>
</mapper>